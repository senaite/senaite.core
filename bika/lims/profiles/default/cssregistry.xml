<?xml version="1.0"?>
<object name="portal_css">

  <!-- Bootstrap CSS -->
  <stylesheet
    id="++resource++senaite.core.bootstrap.static/css/bootstrap.min.css"
    applyPrefix="True"
    media="" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression=""
    insert-before="*"/>

  <!-- Bootstrap Integration CSS (insert at the end) -->
  <stylesheet
    id="++resource++senaite.core.bootstrap.static/css/bootstrap-integration.css"
    applyPrefix="True"
    media="" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression=""
    insert-after="*"/>

  <!-- Core CSS -->
  <stylesheet
    id="bika.lims.graphics.css"
    media="screen"
    rel="stylesheet"
    rendering="link"
    insert-after="*"/>

  <stylesheet
    id="++resource++bika.lims.css/jquery.ui.combogrid.css"
    media="screen"
    rel="stylesheet"
    rendering="link"
    insert-after="*"/>

  <!-- Overwrite default jqueryui.css -->
  <stylesheet
    id="++resource++jquery-ui-themes/sunburst/jqueryui.css"
    media="screen"
    rel="stylesheet"
    rendering="link"
    insert-after="*"/>

  <!-- Activate CSS stylesheets we need -->
  <stylesheet
    id="++resource++plone.app.jquerytools.overlays.css"
    enabled="1"/>

  <!-- Remove broken CSS -->
  <stylesheet remove="True" id="invisible.css"/>

  <!-- Deactivate (Remove?) CSS stylesheets that come with Sunburst theme -->
  <!-- Keep synced with Plone's cssregistry.xml -->
  <stylesheet
    id="invisibles.css"
    enabled="0"/>
  <stylesheet
    id="reset.css"
    enabled="0"/>
  <stylesheet
    id="base.css"
    enabled="0"/>
  <stylesheet
    id="print.css"
    enabled="0"/>
  <stylesheet
    id="authoring.css"
    enabled="0"/>
  <stylesheet
    id="RTL.css"
    enabled="0"/>
  <stylesheet
    id="forms.css"
    enabled="0"/>
  <stylesheet
    id="member.css"
    enabled="0"/>
  <stylesheet
    id="controlpanel.css"
    enabled="0"/>
  <stylesheet
    id="navtree.css"
    enabled="0"/>
  <stylesheet
    id="public.css"
    enabled="0"/>
  <stylesheet
    id="ploneCustom.css"
    enabled="0"/>
  <stylesheet
    id="columns.css"
    enabled="0"/>
  <stylesheet
    id="mobile.css"
    enabled="0"/>
  <stylesheet
    id="IEFixes.css"
    enabled="0"/>
  <stylesheet
    id="deprecated.css"
    enabled="0"/>
  <stylesheet
    id="++resource++plone.app.discussion.stylesheets/discussion.css"
    enabled="0"/>

</object>
