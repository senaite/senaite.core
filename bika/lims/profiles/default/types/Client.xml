         icon_expr="string:${portal_url}/images/supplyorder.png"
         link_target=""
         url_expr="string:${object_url}/orders"
         i18n:attributes="title"
         visible="True">
    <permission value="BIKA: Manage Supply Orders"/>
 </action>

 <action title="Edit"
         action_id="edit"
         category="object"
         url_expr="string:${object_url}/base_edit"
         i18n:attributes="title"
         i18n:domain="plone"
         visible="True">
    <permission value="BIKA: Edit Client"/>
 </action>

 <action title="Contacts"
         action_id="contacts"
         category="object"
         icon_expr="string:${portal_url}/images/client_contact.png"
         url_expr="string:${object_url}/contacts"
         i18n:attributes="title"
         visible="True">
    <permission value="Modify portal content"/>
 </action>

</object>
