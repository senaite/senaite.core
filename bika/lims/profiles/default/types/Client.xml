<?xml version="1.0"?>
<object name="Client"
        meta_type="Factory-based Type Information with dynamic views"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="plone"
        purge="True">
 <property name="title" i18n:translate="">Client</property>
 <property name="description"></property>
 <property name="content_icon">++resource++bika.lims.images/client.png</property>
 <property name="content_meta_type">Client</property>
 <property name="product">bika.lims</property>
 <property name="factory">addClient</property>
 <property name="add_view_expr">string:${object/absolute_url}/base_edit</property>
 <property name="link_target"></property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="ARImport"/>
  <element value="ARTemplate"/>
  <element value="AnalysisProfile"/>
  <element value="AnalysisRequest"/>
  <element value="AnalysisSpec"/>
  <element value="Attachment"/>
  <element value="Batch"/>
  <element value="Contact"/>
  <element value="Sample"/>
  <element value="SamplePoint"/>
  <element value="SRTemplate"/>
  <element value="SupplyOrder"/>
  <element value="Folder"/>
  <element value="SamplingRound"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="immediate_view">base_edit</property>
 <property name="default_view_fallback">False</property>

 <alias from="(Default)" to="base_view"/>
 <alias from="analysisrequests" to="base_view"/>
 <alias from="edit" to="base_edit"/>
 <alias from="view" to="base_view"/>

 <action title="Batches"
         action_id="batches"
         category="object"
         condition_expr=""
         icon_expr="string:${portal_url}/images/batch.png"
         link_target=""
         url_expr="string:${object_url}/batches"
         i18n:attributes="title"
         visible="True">
  <permission value="View"/>
 </action>

 <action title="Analysis Requests"
         action_id="view"
         category="object"
         condition_expr=""
         icon_expr="string:${portal_url}/images/ar.png"
         link_target=""
         url_expr="string:${object_url}/analysisrequests"
         i18n:attributes="title"
         visible="True">
  <permission value="View"/>
 </action>

 <action title="Samples"
         action_id="samples"
         category="object"
         condition_expr=""
         icon_expr="string:${portal_url}/images/sample.png"
         link_target=""
         url_expr="string:${object_url}/samples"
         i18n:attributes="title"
         visible="True">
  <permission value="BIKA: Manage Samples"/>
 </action>

 <action title="Imports"
         action_id="imports"
         category="object"
         condition_expr=""
         icon_expr="string:${portal_url}/images/arimport.png"
         link_target=""
         url_expr="string:${object_url}/arimports"
         i18n:attributes="title"
         visible="True">
  <permission value="BIKA: Manage ARImport"/>
 </action>

 <action title="SamplePoints"
         action_id="samplepoints"
