<?xml version="1.0"?>
<object name="InstrumentLocations"
        meta_type="Factory-based Type Information with dynamic views"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="senaite.core"
        purge="True">

  <property name="title" i18n:translate="">Instrument Locations</property>
  <property name="description"></property>
  <property name="content_icon">++resource++bika.lims.images/instrumenttype.png</property>
  <property name="content_meta_type">InstrumentLocations</property>
  <property name="product">bika.lims</property>
  <property name="factory">addInstrumentLocations</property>
  <property name="add_view_expr"></property>
  <property name="link_target"></property>
  <property name="global_allow">False</property>
  <property name="filter_content_types">True</property>
  <property name="allowed_content_types">
    <element value="InstrumentLocation"/>
  </property>
  <property name="allow_discussion">False</property>
  <property name="default_view_fallback">False</property>

  <alias from="(Default)" to="folder_view"/>
  <alias from="view" to="folder_view"/>
  <alias from="base_view" to="folder_view"/>
  <alias from="edit" to="base_edit"/>

</object>
