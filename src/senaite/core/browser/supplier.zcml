<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="senaite.core">

    <browser:page
      for="senaite.core.interfaces.ISupplier"
      name="contacts"
      class="senaite.core.browser.supplier.ContactsView"
      permission="zope2.View"
      layer="senaite.core.interfaces.IBikaLIMS"
    />

    <browser:page
      for="senaite.core.interfaces.ISupplier"
      name="reference_samples"
      class="senaite.core.browser.supplier.SupplierReferenceSamplesView"
      permission="zope2.View"
      layer="senaite.core.interfaces.IBikaLIMS"
    />

    <browser:page
      for="senaite.core.interfaces.ISupplier"
      name="instruments"
      class="senaite.core.browser.supplier.SupplierInstrumentsView"
      permission="zope2.View"
      layer="senaite.core.interfaces.IBikaLIMS"
    />

</configure>
