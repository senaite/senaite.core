<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="senaite.core">

  <browser:page
      for="*"
      name="ng"
      class="senaite.core.browser.idserver.view.IDServerView"
      permission="cmf.ManagePortal"
      layer="senaite.core.interfaces.IBikaLIMS"
      />

  <browser:page
      for="*"
      name="ng_seed"
      class="senaite.core.browser.idserver.view.IDServerView"
      attribute="seed"
      permission="cmf.ManagePortal"
      layer="senaite.core.interfaces.IBikaLIMS"
      />

  <browser:page
      for="*"
      name="ng_flush"
      class="senaite.core.browser.idserver.view.IDServerView"
      attribute="flush"
      permission="cmf.ManagePortal"
      layer="senaite.core.interfaces.IBikaLIMS"
      />

</configure>
