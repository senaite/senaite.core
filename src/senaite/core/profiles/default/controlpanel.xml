  <!-- SENAITE Registry -->
  <configlet
      title="SENAITE Registry"
      action_id="senaite.core.registry"
      appId="senaite.core"
      category="Products"
      condition_expr=""
      icon_expr="string:++plone++senaite.core.static/assets/svg/setup.svg"
      url_expr="string:${portal_url}/@@senaite-controlpanel"
      visible="True"
      i18n:domain="senaite.core"
      i18n:attributes="title">
    <permission>senaite.core: Manage Bika</permission>
  </configlet>

